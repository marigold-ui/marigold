{
  "name": "@marigold/storybook-config",
  "description": "Marigold's storybook configuration",
  "version": "0.7.23",
  "license": "MIT",
  "type": "module",
  "exports": "./main.js",
  "keywords": [
    "marigold",
    "storybook",
    "config",
    "typescript",
    "react",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marigold-ui/marigold",
    "directory": "config/prettier"
  },
  "dependencies": {
    "@babel/core": "7.20.5",
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.8.1",
    "@marigold/components": "workspace:*",
    "@marigold/theme-b2b": "workspace:*",
    "@marigold/theme-core": "workspace:*",
    "@marigold/theme-unicorn": "workspace:*",
    "@mdx-js/react": "1.6.22",
    "@storybook/addon-a11y": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/jest": "0.0.10",
    "@storybook/testing-library": "0.0.13",
    "@types/react": "18",
    "babel-loader": "8.3.0",
    "chromatic": "^6.6.3",
    "find-up": "5.0.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "webpack": "4"
  },
  "devDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "@storybook/react": "6.5.15",
    "react": "18.x",
    "react-dom": "18.x",
    "typescript": "4.9.4"
  },
  "scripts": {
    "start": "start-storybook -p 1337 -c .",
    "build": "build-storybook --disable-telemetry -c . -o ../../storybook-static",
    "test": "start-storybook --smoke-test --ci -c .",
    "chromatic": "pnpm dlx chromatic --force-rebuild --exit-zero-on-changes",
    "ci": "pnpm dlx chromatic"
  }
}
