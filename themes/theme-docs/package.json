{
  "name": "@marigold/theme-docs",
  "version": "4.1.1",
  "description": "Marigold Docs Theme",
  "license": "MIT",
  "keywords": [
    "marigold",
    "docs",
    "theme",
    "styles"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./styles.css": "./dist/styles.css",
    "./theme.css": "./dist/theme.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marigold-ui/marigold",
    "directory": "themes/theme-docs"
  },
  "devDependencies": {
    "@marigold/tsconfig": "workspace:*",
    "@tailwindcss/postcss": "4.1.18",
    "cross-env": "^7.0.3",
    "cssnano": "7.1.2",
    "postcss-cli": "11.0.1",
    "postcss-prefix-selector": "2.1.1",
    "shx": "^0.3.4",
    "tailwindcss": "4.1.18",
    "tsdown": "0.20.3"
  },
  "dependencies": {
    "@marigold/components": "workspace:*",
    "@marigold/system": "workspace:*",
    "@tailwindcss/typography": "0.5.19",
    "tailwind-scrollbar": "4.0.2",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-react-aria-components": "2.0.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsdown && postcss -o dist/styles.css src/styles.css && shx cp src/theme.css dist/theme.css"
  }
}
