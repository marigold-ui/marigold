{
  "name": "@marigold/components",
  "version": "15.3.0",
  "description": "Components for the Marigold Design System",
  "license": "MIT",
  "keywords": [
    "marigiold",
    "components",
    "design",
    "design system",
    "react"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marigold-ui/marigold",
    "directory": "packages/components"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@marigold/system": "workspace:*",
    "@marigold/types": "workspace:*",
    "@react-aria/button": "^3.14.2",
    "@react-aria/calendar": "^3.9.2",
    "@react-aria/focus": "^3.21.2",
    "@react-aria/i18n": "^3.12.13",
    "@react-aria/interactions": "^3.25.6",
    "@react-aria/label": "^3.7.22",
    "@react-aria/landmark": "^3.0.7",
    "@react-aria/overlays": "^3.30.0",
    "@react-aria/selection": "^3.26.0",
    "@react-aria/ssr": "^3.9.10",
    "@react-aria/table": "^3.17.8",
    "@react-aria/utils": "^3.31.0",
    "@react-aria/visually-hidden": "^3.8.28",
    "@react-stately/collections": "^3.12.8",
    "@react-stately/data": "^3.14.1",
    "@react-stately/table": "^3.15.1",
    "@react-stately/tree": "^3.9.3",
    "@react-types/shared": "^3.32.1",
    "@react-types/table": "^3.13.4",
    "react-aria-components": "^1.13.0",
    "react-stately": "^3.42.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@marigold/icons": "workspace:*",
    "@marigold/tsconfig": "workspace:*",
    "@types/react": "19.1.12",
    "lucide-react": "0.542.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tsdown": "0.15.6"
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "watch": "tsdown src/index.ts --watch",
    "clean": "rm -rf node_modules && rm -rf dist"
  }
