{
  "name": "@marigold/components",
  "version": "15.0.0",
  "description": "Components for the Marigold Design System",
  "license": "MIT",
  "keywords": [
    "marigiold",
    "components",
    "design",
    "design system",
    "react"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marigold-ui/marigold",
    "directory": "packages/components"
  },
  "dependencies": {
    "@internationalized/date": "3.9.0",
    "@marigold/system": "workspace:*",
    "@marigold/types": "workspace:*",
    "@react-aria/button": "3.14.1",
    "@react-aria/calendar": "3.9.1",
    "@react-aria/focus": "3.21.1",
    "@react-aria/i18n": "3.12.12",
    "@react-aria/interactions": "3.25.5",
    "@react-aria/label": "3.7.21",
    "@react-aria/landmark": "3.0.6",
    "@react-aria/overlays": "3.29.1",
    "@react-aria/selection": "3.25.1",
    "@react-aria/ssr": "3.9.10",
    "@react-aria/table": "3.17.7",
    "@react-aria/utils": "3.30.1",
    "@react-aria/visually-hidden": "3.8.27",
    "@react-stately/collections": "3.12.7",
    "@react-stately/data": "3.14.0",
    "@react-stately/table": "3.15.0",
    "@react-stately/tree": "3.9.2",
    "@react-types/shared": "3.32.0",
    "@react-types/table": "3.13.3",
    "react-aria-components": "1.12.1",
    "react-stately": "3.41.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@marigold/icons": "workspace:*",
    "@marigold/tsconfig": "workspace:*",
    "@types/react": "19.1.12",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tsup": "8.5.0"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "watch": "tsup src/index.ts --watch",
    "clean": "rm -rf node_modules && rm -rf dist"
  }
}
